{
  "name": "GrpcJSServiceApi",
  "version": "0.0.1",
  "private": true,
  "description": "NestJS gRPC service API equivalent to .NET GrpcServiceApi",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "proto:ts": "grpc_tools_node_protoc --plugin=protoc-gen-ts=$(npm bin)/protoc-gen-ts --ts_out=src/interfaces --js_out=import_style=commonjs,binary:src/interfaces --grpc_out=grpc_js:src/interfaces -I ./src/protos ./src/protos/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.7",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/node": "^20.0.0",
    "grpc-tools": "^1.12.4",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.164.0",
    "tslib": "^2.6.2",
    "tslint": "^6.1.3",
    "typescript": "^5.4.0"
  }
}
